{
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "none",
  "importOrder": [
    "<BUILT_IN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "services",
    "",
    "contexts",
    "",
    "hooks",
    "",
    "helpers",
    "",
    "constants",
    "",
    "routes",
    "",
    "features",
    "",
    "components",
    "",
    "assets",
    "",
    "^[.]"
  ],
  "plugins": [
    "prettier-plugin-organize-imports",
    "@ianvs/prettier-plugin-sort-imports"
  ]
}
